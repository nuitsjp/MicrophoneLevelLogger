<Window x:Class="Quietrum.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Quietrum.View"
        xmlns:ScottPlot="clr-namespace:ScottPlot;assembly=ScottPlot.WPF"
        xmlns:viewModel="clr-namespace:Quietrum.ViewModel;assembly=Quietrum.ViewModel"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance {x:Type viewModel:MainWindowViewModel}, IsDesignTimeCreatable=True}"
        Title="MainWindow" Height="1024" Width="1280">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="{Binding Elapsed}"/>
        <ScottPlot:WpfPlot Grid.Row="1" x:Name="WpfPlot1" >
            
        </ScottPlot:WpfPlot>
    </Grid>
</Window>
